{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "store",
      "env": {
        "ENVIRONMENT": "dev",
        "EAS_BUILD_PROFILE": "development",
        "SENTRY_ORG": "eleva-labs",
        "SENTRY_PROJECT": "chatscommerce"
      },
      "environment": "development",
      "channel": "development",
      "android": {
        "image": "latest",
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleDebug",
        "env": {
          "ENVIRONMENT": "dev"
        }
      },
      "ios": {
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "ENVIRONMENT": "prod",
        "SENTRY_ORG": "eleva-labs",
        "SENTRY_PROJECT": "chatscommerce"
      },
      "environment": "production",
      "channel": "production",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "development": {
      "android": {
        "serviceAccountKeyPath": "./credentials/android/service-account-key.json",
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6749920971",
        "appleTeamId": "TGB3VRK9LR"
      }
    },
    "production": {
      "android": {
        "serviceAccountKeyPath": "./credentials/android/service-account-key.json",
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6749920971"
      }
    }
  }
}
