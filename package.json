    "@react-navigation/native-stack": "^6.10.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react-native": "^5.24.3",
    "@shopify/flash-list": "^1.5.0",
    "axios": "^1.6.4",
    "date-fns": "2.21.1",
